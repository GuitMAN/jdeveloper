<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<AuditConfig componentType="OWSM-JKSMBEAN" xmlns="http://xmlns.oracle.com/ias/audit/audit.xsd">
	<Events componentType="OWSM-JKSMBEAN"
		displayName="Oracle Web Services Manager - JKS Store Mbean" isLibrary="true">
		<Category name="ImportCertificate">
			<Event name="ImportCertificate">
				<HelpText>Generated when certificate is imported into JKS keystore</HelpText>
				<Attribute displayName="certificate alias" maxLength="255"
					name="Alias" shortName="alias" type="string">
					<HelpText>Alias of the certificate to be imported</HelpText>
				</Attribute>
				<Attribute displayName="KeyStore Entry Type" maxLength="255"
					name="keystoreEntryType" shortName="keystoreEntryType" type="string">
					<HelpText>Keystore entry type</HelpText>
				</Attribute>
			</Event>
		</Category>
		<Category name="DeleteKeyStoreEntry">
			<Event name="DeleteKeyStoreEntry">
				<HelpText>Generated when certificate is deleted from JKS keystore</HelpText>
				<Attribute displayName="DeletedAlias" maxLength="255" name="DeletedAlias"
					shortName="DeletedAlias" type="string">
					<HelpText>Alias of the entry to be deleted</HelpText>
				</Attribute>
			</Event>
		</Category>
	</Events>
	<Levels>
		<Level name="ExceptionsOnly">
		</Level>
		<Level name="Basic">
			<FilterEvent enabled="true" eventName="ImportCertificate" />
			<FilterEvent enabled="true" eventName="DeleteKeyStoreEntry" />
		</Level>
		<Level name="Enhanced">
		</Level>
	</Levels>
	<FilterPresetDefinitions>
		<FilterPresetDefinition helpText="Low filter preset ."
			name="Low">ImportCertificate,DeleteKeyStoreEntry
		</FilterPresetDefinition>
                <FilterPresetDefinition helpText="Medium  filter preset ."
                        name="Medium">ImportCertificate,DeleteKeyStoreEntry
                </FilterPresetDefinition>
                <FilterPresetDefinition helpText="High filter preset ."
                        name="High">ImportCertificate,DeleteKeyStoreEntry
                </FilterPresetDefinition>
	</FilterPresetDefinitions>
	
</AuditConfig>
