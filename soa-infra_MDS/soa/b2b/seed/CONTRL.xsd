			<xsd:attribute name="ID" fixed="S010" type="xsd:string"/>
			<xsd:attribute name="Type" default="Composite" type="xsd:string"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Element-0070">
		<xsd:annotation>
			<xsd:documentation>Sequence of transfers</xsd:documentation>
			<xsd:documentation>Number assigned by the sender indicating the numerical sequence of one or more transfers.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:restriction base="EDI-Element-Decimal-Type">
					<xsd:attribute name="Name" fixed="Sequence of transfers" type="xsd:string"/>
					<xsd:attribute name="ID" fixed="0070" type="xsd:string"/>
					<xsd:attribute name="EDIDataType" fixed="n" type="xsd:string"/>
				</xsd:restriction>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Element-0073" nillable="true">
		<xsd:annotation>
			<xsd:documentation>First and last transfer</xsd:documentation>
			<xsd:documentation>Indication used for the first and last message in a sequence of the same type of message relating to the same topic.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:simpleContent>
				<xsd:restriction base="EDI-Element-String-Type">
					<xsd:enumeration value="C"/>
					<xsd:enumeration value="F"/>
					<xsd:attribute name="Name" fixed="First and last transfer" type="xsd:string"/>
					<xsd:attribute name="ID" fixed="0073" type="xsd:string"/>
					<xsd:attribute name="EDIDataType" fixed="a" type="xsd:string"/>
				</xsd:restriction>
			</xsd:simpleContent>
		</xsd:complexType>
	</xsd:element>
