			<xsd:attribute name="ID" fixed="UCI" type="xsd:string"/>
			<xsd:attribute name="Type" default="Segment" type="xsd:string"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Element-0020">
		<xsd:annotation>
			<xsd:documentation>Interchange control reference</xsd:documentation>
